{
  "extends": "airbnb",
  "rules": {
    "quotes": 0,
    "react/jsx-filename-extension": 0,
    "eol-last": 0,
    "import/no-unresolved": 0,
    "jsx-quotes": 0,
    "no-confusing-arrow": 0,
    "react/no-danger": 0,
    "object-curly-newline": 0
  },
  "globals": {
    "document": true,
    "window": true
  },
}